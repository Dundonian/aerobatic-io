{
  "shim": {
    "angular": {
      "exports": "angular"
    },
    "angular-route": {
      "deps": ["angular"]
    },
    "angular-animate": {
      "deps": ["angular"]
    },
    "jquery": {
      "exports": "$"
    },
    "jquery-flexslider": {
      "deps": ["jquery"]
    },
    "jquery-scrollto": {
      "deps": ["jquery"]
    },
    "jquery-localscroll": {
      "deps": ["jquery", "jquery-scrollto"]
    },
    "jquery-stellar": {
      "deps": ["jquery"]
    },
    "jquery-owl-carousel": {
      "deps": ["jquery"]
    }
  },
  "paths": {
    "modernizr": ["//cdnjs.cloudflare.com/ajax/libs/modernizr/2.7.1/modernizr.min"],
    "angular": ["//ajax.googleapis.com/ajax/libs/angularjs/1.2.15/angular.min"],
    "angular-route": ["//ajax.googleapis.com/ajax/libs/angularjs/1.2.15/angular-route.min"],
    "angular-animate": ["//ajax.googleapis.com/ajax/libs/angularjs/1.2.15/angular-animate.min"],
    "jquery": ["//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min"],
    "respond": ["//cdnjs.cloudflare.com/ajax/libs/respond.js/1.4.2/respond.min"],
    "jquery-flexslider": ["//cdnjs.cloudflare.com/ajax/libs/flexslider/2.2.2/jquery.flexslider-min"],
    "jquery-scrollto": ["//cdnjs.cloudflare.com/ajax/libs/jquery-scrollTo/1.4.11/jquery.scrollTo.min"],
    "jquery-localscroll": ["//cdnjs.cloudflare.com/ajax/libs/jquery-localScroll/1.3.5/jquery.localScroll.min"],
    "jquery-stellar": ["//cdnjs.cloudflare.com/ajax/libs/stellar.js/0.6.2/jquery.stellar.min"],
    "jquery-owl-carousel": ["//cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.32/owl.carousel.min"]
  },
  "mainModules": {
    "index": "js/app"
  }
}
